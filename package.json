    "axios": "^1.7.7",
    "highlight.js": "^11.10.0",
    "lodash": "^4.17.21",
    "markdown-it-anchor": "^9.2.0",
    "medium-article-api": "^1.0.4",
    "moment": "^2.30.1",
    "pinia": "^2.2.6",
    "quasar": "^2.17.2",
    "v-timeline-component": "^0.2.2",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.5.12",
    "vue-markdown-render": "^2.2.1",
    "vue-resize": "^1.0.1",
    "vue-resizer": "^1.1.9",
    "vue-router": "^4.4.5",
    "webpack": "^5.96.1"
